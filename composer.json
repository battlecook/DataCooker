{
  "name": "battlecook/datacooker",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "description": "It's project for different kinds of data store. \n\nIt provides an interface by abstracting individual libraries (\\ pdo, \\ Redis, \\ Memcached, etc.) that access data stores.\n",
  "require": {
    "php": "^7.1.0"
  },
  "suggest": {
    "phpoffice/phpspreadsheet": "Needed to support spreadsheet",
    "ext-pdo": "Needed to support RDB",
    "ext-memcached": "Needed to support memcached",
    "ext-redis": "Needed to support redis",
    "ext-apcu": "Needed to support apcu"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "battlecook\\": "src",
      "test\\": "test"
    }
  }
}